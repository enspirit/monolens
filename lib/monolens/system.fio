@import finitio/data

Lenses.File = .Monolens::Lens::File <info> {
  version: String
  lenses: Lens.Chain
}

Lens.Chain = .Monolens::Lens::Chain <lenses>
  [Lens.Transform
  |Lens.Leaf]

Lens.Transform = .Monolens::Lens::Transform <hash> {
  transform: { ... : Lens.Chain }
}

Lens.Leaf = .Monolens::Lens <leaf> String
